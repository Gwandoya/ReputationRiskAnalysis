<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem mnemonicParsing="false" text="New File" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Load File" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem disable="true" mnemonicParsing="false" text="Save " />
                        <MenuItem disable="true" mnemonicParsing="false" text="Save As.." />
                        <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="View">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Editing" />
                        <MenuItem disable="true" mnemonicParsing="false" text="Graphs" />
                        <MenuItem disable="true" mnemonicParsing="false" text="Compare Results" />
                    </items>
                  </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                        <MenuItem fx:id="guideMenuBtn" disable="true" mnemonicParsing="false" text="Guide" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox />
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Random ass text" />
         </children>
      </HBox>
   </bottom>
   <left>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <TreeView fx:id="treeView" prefHeight="455.0" prefWidth="102.0" />
            <Button fx:id="addStkBtn" mnemonicParsing="false" onAction="#handleAddStkBtn" prefHeight="25.0" prefWidth="150.0" text="Add New Stakeholder">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="editStkBtn" layoutX="10.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleEditStkBtn" text="Edit Stakeholder">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="deleteStkBtn" contentDisplay="BOTTOM" layoutX="10.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleDeleteStkBtn" text="Delete Stakeholder">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <center>
      <ScrollPane prefViewportHeight="558.0" prefViewportWidth="700.0">
         <content>
            <GridPane alignment="CENTER" prefHeight="558.0" prefWidth="700.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="StkNameLbl" alignment="CENTER_RIGHT" text="Stakeholder Name:" />
                  <TextField fx:id="stkNameTextView" promptText="Stakeholder Name" GridPane.columnIndex="1" />
                  <Label fx:id="wrongNameLbl" alignment="CENTER_RIGHT" layoutX="185.0" layoutY="58.0" prefHeight="17.0" prefWidth="22.0" textFill="RED" visible="false" GridPane.columnIndex="2" />
               </children>
            </GridPane>
         </content>
      </ScrollPane>
   </center>
</BorderPane>

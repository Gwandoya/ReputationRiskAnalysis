                                    <AnchorPane BorderPane.alignment="CENTER">
                                       <children>
                                          <Label layoutX="14.0" layoutY="72.0" text="Name:">
                                             <font>
                                                <Font name="System Bold" size="14.0" />
                                             </font>
                                          </Label>
                                          <Label layoutX="14.0" layoutY="175.0" text="Description:">
                                             <font>
                                                <Font name="System Bold" size="14.0" />
                                             </font>
                                          </Label>
                                          <TextField fx:id="stkTextField" layoutX="169.0" layoutY="70.0" promptText="Enter name..." />
